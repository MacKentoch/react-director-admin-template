{
  "name": "react-director-admin-template",
  "version": "0.6.0",
  "description": "ReactJS version of Director Responsive Admin Template Free",
  "main": "src/index.js",
  "scripts": {
    "flow": "flow",
    "test": "cross-env NODE_ENV=test jest",
    "test:update": "cross-env NODE_ENV=test jest -u",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "start": "cross-env NODE_ENV=dev node ./server.hot.reload.js",
    "dev": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev-server": "npm run dev && node src/server",
    "prod-server": "npm run prod && node src/server"
  },
  "keywords": [
    "director",
    "admin",
    "template",
    "react",
    "redux",
    "react-redux",
    "ES6",
    "ES2015",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools-extension"
  ],
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
